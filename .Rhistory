cria.dataset <- function(perguntas, alunos){
dados <- matrix(data = trunc(runif(perguntas * alunos, min = -3, max = 3)), nrow = alunos, ncol = perguntas)
colnames(dados) <- c(
paste("P", 1:perguntas, sep=""))
rownames(dados) <- paste("a", 1:alunos, sep = " ")
}
cria.dataset(38, 300)
View(cria.dataset)
cria.dataset <- function(perguntas, alunos){
dados <- matrix(data = trunc(runif(perguntas * alunos, min = -3, max = 3)), nrow = alunos, ncol = perguntas)
colnames(dados) <- c(
paste("P", 1:perguntas, sep=""))
rownames(dados) <- paste("a", 1:alunos, sep = " ")
return(dados)
}
cria.dataset(38, 300)
